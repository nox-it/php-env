{
    "name": "nyx-solutions/php-nyx-env",
    "description": "PHP NYX ENV",
    "type": "library",
    "keywords": ["php", "environment"],
    "license": "BSD-3-Clause",
    "version": "2.0.0",
    "authors": [
        {
            "name": "NYX Solutions",
            "email": "nyx@jsas.com.br"
        },
        {
            "name": "Jonatas Sas",
            "email": "atendimento@jsas.com.br"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "ext-mbstring": "*",

        "vlucas/phpdotenv": "~4.1.5",

        "nyx-solutions/php-nyx-helpers": "~4.0.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "nyx\\base\\env\\": "env/"
        }
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "generate-php-fpm-env-vars": "./bin/generate-php-fpm-env-vars"
    },
    "scripts-descriptions": {
        "generate-php-fpm-env-vars": "Generates PHP/FPM Environment Variables based on the System Environment Variables."
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nyx-solutions/php-nyx-env"
        }
    ]
}
